<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_alumno_form" model="ir.ui.view">
            <field name="name">alumno.form</field>
            <field name="model">alumno.model</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="nombre" />
                            <field name="apellido" />
                            <field name="legajo" />
                            <field name="fecha_nacimiento" />
                            <field name="email" />
                            <field name="telefono" />
                            <field name="pais" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>