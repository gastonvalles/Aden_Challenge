<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_programa_form" model="ir.ui.view">
            <field name="name">programa.form</field>
            <field name="model">programa.model</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="nombre" />
                            <field name="descripcion" />
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>